#include "MsgQueue.h"
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
MsgQueue::MsgQueue()
{
	this->init();
}
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
MsgQueue::~MsgQueue()
{
	this->destory();
}
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
void MsgQueue::init()
{
	this->mQueueId=get_queue(mQueueKey);
}
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
void MsgQueue::destory()
{
	remove_queue(mQueueId);
}
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
int  MsgQueue::snd_queue_buff(void * _data)
{
	return 	snd_queue(m_msgid_img_buff,m_img_buff_type,_data);
}
/*-----------------------------------*/
