#include "MsgQueue.h"
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
MsgQueue::MsgQueue()
{
	this->init();
}
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
MsgQueue::~MsgQueue()
{
	this->destory();
}
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
void MsgQueue::init()
{
	this->mQueueId=get_queue(mQueueKey);
}
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
void MsgQueue::destory()
{
	remove_queue(mQueueId);
}
/*-----------------------------------*/
 /**
  *
 */
/*-----------------------------------*/
